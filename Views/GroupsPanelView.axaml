<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="using:Paint2.ViewModels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Paint2.Views.GroupsPanelView"
             x:DataType="vm:GroupsPanelViewModel">
    <StackPanel Background="DarkSalmon" Width="300">
        <Button Content="Добавить элемент"
                Command="{Binding AddElementCommand}"/>
        <TreeDataGrid Source="{Binding TreeSource}"/>
    </StackPanel>
</UserControl>
